def add_qx(func):
    print("开始装饰权限")

    def call_fun(*args, **kargs):
        print("权限验证一")
        print("权限验证一")
        func(*args, **kargs)
    return call_fun


def set_func(func):
    print("开始装饰登陆")

    def call_fun(*args, **kargs):
        print("权限验证一")
        print("权限验证一")
        func(*args, **kargs)
    return call_fun

@add_qx
@set_func
def login():
    print("***************test1******************")

login()